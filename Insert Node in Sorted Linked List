"""
Definition of ListNode
class ListNode(object):

    def __init__(self, val, next=None):
        self.val = val
        self.next = next
"""


class Solution:
    """
    @param: head: The head of linked list.
    @param: val: An integer.
    @return: The head of new linked list.
    """
    def insertNode(self, head, val):
        # write your code here
        tmp = ListNode(val)
        if head is None:
            return tmp
        if head.next is None:
            if head.val > val:
                tmp.next = head
                return tmp
            else:
                head.next = tmp
                return head
        dummyNode = head
        while head.next != None:
            if head.val <= val and head.next.val >=val:
                dummy = head.next
                head.next = tmp
                tmp.next = dummy
                return dummyNode
            elif head.val > val:
                tmp.next = head
                return tmp
            else:
                head = head.next
        head.next = tmp
        return dummyNode
