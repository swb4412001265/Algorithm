class Solution:
    """
    @param: s: A string
    @return: whether the string is a valid parentheses
    """
    def isValidParentheses(self, s):
        # write your code here
        stack = []
        for c in s:
            if c in ['(', '{', '[']:
                stack.append(c)
                continue
            if len(stack) == 0:
                return False
            if c == ')' and stack[-1] != '(':
                return False
            if c == ']' and stack[-1] != '[':
                return False
            if c == '}' and stack[-1] != '{':
                return False
            stack.pop(len(stack) - 1)
        
        if len(stack) == 0:
            return True
        else:
            return False
