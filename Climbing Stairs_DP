class Solution:
    """
    @param n: An integer
    @return: An integer
    """
    def climbStairs(self, n):
        # write your code here
        #if there is 0 stairs, so there is 0 way
        if n == 0:
            return 0
        f = [0 for i in range(n+1)]
        f[0] = 1
        f[1] = 1
        
        #define f[n] as there are f[n] distinct ways tp climb to stair n
        for i in range(2, n+1):
            f[i] = f[i -1] + f[i -2]
        return f[n]
