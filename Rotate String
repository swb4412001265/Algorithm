class Solution:
    """
    @param str: An array of char
    @param offset: An integer
    @return: nothing
    """
    def rotateString(self, str, offset):
        # write your code here
        if len(str) == 0:
            return
        
        offset = offset % len(str)
        
        tmp = []
        for i in range(len(str)):
            tmp.append(str[i])
        print(tmp)
            
        for i in range(len(str)):
            str[i] = tmp[(i - offset) % len(str)]
